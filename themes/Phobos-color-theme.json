    "tokenColors": [
		{
			"name": "Dark Grey",
			"scope": [
				"comment",
                "punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5b5b5b"
			}
		},
		{
			"name": "Light Grey",
			"scope": [
				"keyword.control",
                "variable",
                "keyword.other"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
        {
            "name": "Grey",
            "scope": [
                "keyword.control.directive",
                "punctuation.definition.directive",
                "storage",
                "support.type",
                "keyword.operator.assignment",
                "keyword.operator.comparison",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#aaaaaa"
            }
        },
		{
            "name": "Yellow",
            "scope": [
                "punctuation.section",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.definition",
                "entity.name.section"

            ],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "name": "Lime",
            "scope": [
                "keyword.other.unit",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#d7e092"
            }
        },
        {
            "name": "White",
            "scope": [
                "entity.name.function",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Red",
            "scope": [
                "keyword.operator",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#ee5d43"
            }
        },
        {
            "name": "Green",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#6db639"
            }
        },
	]
}
